{
  "name": "expresslogger/api",
  "description": "ExpressLogger Interfaces",
  "license": "MIT",
  "authors": [
    {
      "name": "Konstantin Deryabin",
      "email": "kderyabin@orange.fr"
    }
  ],
  "require": {
    "php": ">=7.4"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "~3.5.8"
  },
  "autoload": {
    "psr-4": {
      "ExpressLogger\\API\\": "src/"
    }
  },
  "scripts": {
    "cs": "./vendor/bin/phpcs --colors --encoding=utf-8 --standard=PSR12 --runtime-set ignore_warnings_on_exit 1  ./src",
    "cs-fix": "./vendor/bin/phpcbf --encoding=utf-8 --standard=PSR12  -n --runtime-set ignore_warnings_on_exit 1 ./src"
  }
}